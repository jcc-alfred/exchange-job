{
  "apps": [
    {
      "name": "GetBlockAddressBitnetTask",
      "script": "./core/Tasks/GetBlockAddress/BitnetTask.js",
      "exec_mode": "fork",
      "watch": [
        "core"
      ],
      "ignore_watch": [
        "./core/Tasks/Deposit/BlockNumber",
        "./core/Tasks/Deposit/GTBBlockNumber",
        "./core/Tasks/Deposit/ETCBlockNumber"
      ],
      "watch_options": {
        "usePolling": true
      },
      "merge_logs": true,
      "out_file": "./logs/GetBlockAddress/BitnetTask.out.log",
      "error_file": "./logs/GetBlockAddress/BitnetTask.err.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
    },
